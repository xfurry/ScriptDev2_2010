+        {
+            m_creature->AddThreat(who, 0.0f);
+            m_creature->SetInCombatWith(who);
+            who->SetInCombatWith(m_creature);
+            DoStartMovement(who);
+        }
+    }
+
+    void MoveInLineOfSight(Unit* who)
+    {
+        if (!who)
